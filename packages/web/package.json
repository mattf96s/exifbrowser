{
  "name": "@exifbrowser/web",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "packageManager": "pnpm@8.12.1",
  "engines": {
    "node": "20",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "pnpm run clean && remix vite:build && pnpm run worker && pnpm run tsworker",
    "worker": "esbuild ./workers/*.js --bundle --outdir=./public/workers",
    "tsworker": "esbuild ./workers/*.ts --bundle --outdir=./public/workers",
    "dev": "pnpm run clean && sst bind 'node ./server.mjs'",
    "start": "cross-env NODE_ENV=production node ./server.mjs",
    "clean": "rimraf build",
    "nuke": "npm run clean && rimraf node_modules package-lock.json && npm i",
    "typecheck": "tsc",
    "clean-build-and-cache": "rm -rf ./.cache && rm -rf ./build && rm -rf ./public/build",
    "format": "prettier --write .",
    "lint": "eslint .",
    "validate": "run-p \"test -- --run\" lint typecheck",
    "knip": "knip"
  },
  "lint-stage": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*": [
      "prettier --ignore-unknown"
    ]
  },
